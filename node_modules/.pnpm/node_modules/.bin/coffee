#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nick/Documents/Med Sig Builder/Med-Sig-Builder/EHR-CRM-System/node_modules/.pnpm/coffeescript@2.7.0/node_modules/coffeescript/bin/node_modules:/Users/nick/Documents/Med Sig Builder/Med-Sig-Builder/EHR-CRM-System/node_modules/.pnpm/coffeescript@2.7.0/node_modules/coffeescript/node_modules:/Users/nick/Documents/Med Sig Builder/Med-Sig-Builder/EHR-CRM-System/node_modules/.pnpm/coffeescript@2.7.0/node_modules:/Users/nick/Documents/Med Sig Builder/Med-Sig-Builder/EHR-CRM-System/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nick/Documents/Med Sig Builder/Med-Sig-Builder/EHR-CRM-System/node_modules/.pnpm/coffeescript@2.7.0/node_modules/coffeescript/bin/node_modules:/Users/nick/Documents/Med Sig Builder/Med-Sig-Builder/EHR-CRM-System/node_modules/.pnpm/coffeescript@2.7.0/node_modules/coffeescript/node_modules:/Users/nick/Documents/Med Sig Builder/Med-Sig-Builder/EHR-CRM-System/node_modules/.pnpm/coffeescript@2.7.0/node_modules:/Users/nick/Documents/Med Sig Builder/Med-Sig-Builder/EHR-CRM-System/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../coffeescript/bin/coffee" "$@"
else
  exec node  "$basedir/../coffeescript/bin/coffee" "$@"
fi
